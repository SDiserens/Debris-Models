{
  "scenarioFilename": "Asteroids.json", // "JovianMoons.json" , "Asteroids.json",
  "probabilityOutput": 1, // Boolian 0 or 1 to switch probability output on/off
  "relativeGravity": 0, // Boolian 0 or 1 to swithc use of relative gravity between objects
  "outputPrinting": true, // true or false to print output rates
  "individualOutput": true, // true or false to store pair specific collision rates
  "cubeDimension": 0.01, // As fraction of average semiMajor Axis - can be 1 or multiple
  "stepSize": 1, // size of timestep in days
  "randomSeed": null, // integer seed value or null to use system time
  "numberEvaluations": 1e6, // Number of timesteps to be evaluated
  "stepsPerEvaluation": 730 // Number of evaluations per section - sensible size is 365
}